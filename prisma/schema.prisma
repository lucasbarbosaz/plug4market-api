generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model alergenicos {
  id                 Int      @id @default(autoincrement())
  nome               String   @db.Text
  nomebr             String   @db.Text
  referencia         String   @db.Text
  data               DateTime @default(now()) @db.Timestamp(0)
  usuario_id         Int
  concentracaomaxima Decimal  @default(100.0000) @db.Decimal(10, 4)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model ambientecentral {
  id     Int
  nome   String? @db.VarChar(200)
  status Int?

  @@ignore
}

model anuncio {
  id         Int      @id @default(autoincrement())
  nome       String   @db.Text
  autor      String   @db.Text
  texto      String   @db.Text
  data       DateTime @default(now()) @db.Timestamp(0)
  status_id  Int
  vencimento DateTime @db.DateTime(0)
}

model apimigrations {
  id          Int       @id @default(autoincrement())
  key         String    @db.VarChar(200)
  nome        String?   @db.VarChar(200)
  informacoes String?   @db.LongText
  data        DateTime? @db.DateTime(0)
  status      Int?
  response    String    @db.VarChar(200)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model asaas_cobrancas {
  id                    Int      @id @default(autoincrement())
  asaas_payment_id      String   @unique(map: "asaas_payment_id") @db.VarChar(255)
  subconta_config_id    Int
  cliente_final_id      Int
  referencia            String?  @db.VarChar(255)
  valor                 Decimal  @db.Decimal(10, 2)
  data_vencimento       DateTime @db.Date
  tipo_pagamento        String   @db.VarChar(50)
  descricao             String?  @db.Text
  status                String   @default("PENDING") @db.VarChar(50)
  transactionReceiptUrl String?  @db.Text
  url_cobranca          String?  @db.Text
  json_asaas            String?  @db.LongText
  created_at            DateTime @default(now()) @db.Timestamp(0)
  updated_at            DateTime @default(now()) @db.Timestamp(0)
  pedido_id             Int?
  financeiro_conta_id   Int?

  @@index([cliente_final_id], map: "fk_cobranca_cliente")
  @@index([subconta_config_id], map: "fk_cobranca_subconta")
  @@index([financeiro_conta_id], map: "idx_financeiro_conta_id")
  @@index([pedido_id], map: "idx_pedido_id")
}

model asaas_config {
  id                    Int     @id @default(autoincrement())
  asaas_id_account      String? @db.VarChar(255)
  cnpj                  String? @db.VarChar(50)
  api_key               String? @db.VarChar(255)
  notifications_enabled Int?    @default(0)
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model asaas_refunds {
  id                    Int       @id @default(autoincrement())
  asaas_refund_id       String    @unique(map: "asaas_refund_id") @db.VarChar(255)
